{
    "AccountService": {
        "Type": "http",
        "Login": {
            "Type": "POST",
            "Async": true,
            "Content": "application/json",
            "Path": "/logic/account/login"
        },
        "Register": {
            "Type": "POST",
            "Async": true,
            "Content": "application/json",
            "Path": "/logic/account/register"
        }
    },
    "GateService": {
