        },
        "CallClient": {
            "Async": false,
            "Type": "Server",
            "CallWay": "Net",
            "Request": "c2s.Rpc.Call"
        },
        "BroadCast": {
            "Async": false,
            "Type": "Server",
            "CallWay": "Net",
            "Request": "s2s.GateBroadCast.Request"
        },
        "Auth": {
            "Async": true,
