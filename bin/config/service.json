            "Async": false,
            "Type": "Client",
            "CallWay": "Net",
            "Request": "c2s.Chat.Request"
        },
        "Test": {
            "Async": false,
            "Type": "Client",
            "CallWay": "Net",
            "Request": "lua.Test",
            "Response": "lua.Test"
        }
